#=============================================================================
# Copyright 2014 Kurento
#
#=============================================================================

set(PACKAGE_VERSION "@PROJECT_VERSION@")
set(KMSOPENCVPLUGINSAMPLE_VERSION ${PACKAGE_VERSION})

message (STATUS "Looking for KMSOPENCVPLUGINSAMPLE: @PROJECT_VERSION@")

include (GenericFind)

generic_find (
  REQUIRED
  LIBNAME KMSCORE
  VERSION ^6.0.0
)

generic_find (
  REQUIRED
  LIBNAME KMSELEMENTS
  VERSION ^6.0.0
)

generic_find (
  REQUIRED
  LIBNAME KMSFILTERS
  VERSION ^6.0.0
)

set (REQUIRED_VARS
  KMSOPENCVPLUGINSAMPLE_VERSION
  KMSOPENCVPLUGINSAMPLE_INCLUDE_DIRS
  KMSOPENCVPLUGINSAMPLE_LIBRARY
  KMSOPENCVPLUGINSAMPLE_LIBRARIES
)

set (KMSOPENCVPLUGINSAMPLE_BINARY_DIR_PREFIX "build" CACHE PATH "Path prefix used to look for binary files")
set (KMSOPENCVPLUGINSAMPLE_SOURCE_DIR_PREFIX "" CACHE PATH "Path prefix used to look for source files")

set(KMSOPENCVPLUGINSAMPLE_INCLUDE_DIRS
  ${KMSCORE_INCLUDE_DIRS}
  ${KMSELEMENTS_INCLUDE_DIRS}
  ${KMSFILTERS_INCLUDE_DIRS}
)

if (NOT "@_INTERFACE_GENERATED_HEADERS@ @_INTERFACE_INTERNAL_GENERATED_HEADERS@" STREQUAL " ")
  if (TARGET kmsopencvpluginsampleinterface)
    set (KMSOPENCVPLUGINSAMPLE_INTERFACE_INCLUDE_DIR "@_INTERFACE_HEADERS_DIR@")
  else ()
    find_path(KMSOPENCVPLUGINSAMPLE_INTERFACE_INCLUDE_DIR
      NAMES
        @_INTERFACE_GENERATED_HEADERS@
        @_INTERFACE_INTERNAL_GENERATED_HEADERS@
      PATH_SUFFIXES
        @_INTERFACE_HEADERS_DIR@
        kurento/modules/opencvpluginsample
    )
  endif ()

  list (APPEND KMSOPENCVPLUGINSAMPLE_INCLUDE_DIRS ${KMSOPENCVPLUGINSAMPLE_INTERFACE_INCLUDE_DIR})
  list (APPEND REQUIRED_VARS KMSOPENCVPLUGINSAMPLE_INTERFACE_INCLUDE_DIR)
endif ()

if (NOT "@_SERVER_INTERNAL_GENERATED_HEADERS@" STREQUAL "")
  if (TARGET kmsopencvpluginsampleimpl)
    set (KMSOPENCVPLUGINSAMPLE_IMPLEMENTATION_INTERNAL_INCLUDE_DIR "@_SERVER_INTERNAL_GENERATED_HEADERS_DIR@")
  else ()
    find_path(KMSOPENCVPLUGINSAMPLE_IMPLEMENTATION_INTERNAL_INCLUDE_DIR
      NAMES
        @_SERVER_INTERNAL_GENERATED_HEADERS@
      PATH_SUFFIXES
        @_SERVER_INTERNAL_GENERATED_HEADERS_DIR@
        kurento/modules/opencvpluginsample
    )
  endif ()

  list (APPEND KMSOPENCVPLUGINSAMPLE_INCLUDE_DIRS ${KMSOPENCVPLUGINSAMPLE_IMPLEMENTATION_INTERNAL_INCLUDE_DIR})
  list (APPEND REQUIRED_VARS KMSOPENCVPLUGINSAMPLE_IMPLEMENTATION_INTERNAL_INCLUDE_DIR)
endif ()

if (NOT "@_SERVER_GENERATED_HEADERS@" STREQUAL "")
  if (TARGET kmsopencvpluginsampleimpl)
    set (KMSOPENCVPLUGINSAMPLE_IMPLEMENTATION_GENERATED_INCLUDE_DIR "${KMSOPENCVPLUGINSAMPLE_SOURCE_DIR_PREFIX}/@_PARAM_SERVER_STUB_DESTINATION@")
  else ()
    find_path(KMSOPENCVPLUGINSAMPLE_IMPLEMENTATION_GENERATED_INCLUDE_DIR
      NAMES
        @_SERVER_GENERATED_HEADERS@
      PATH_SUFFIXES
        @_PARAM_SERVER_STUB_DESTINATION@
        kurento/modules/opencvpluginsample
    )
  endif ()

  list (APPEND KMSOPENCVPLUGINSAMPLE_INCLUDE_DIRS ${KMSOPENCVPLUGINSAMPLE_IMPLEMENTATION_GENERATED_INCLUDE_DIR})
  list (APPEND REQUIRED_VARS KMSOPENCVPLUGINSAMPLE_IMPLEMENTATION_GENERATED_INCLUDE_DIR)
endif()

if (NOT "@_PARAM_SERVER_IMPL_LIB_EXTRA_HEADERS@" STREQUAL "")
  if (TARGET kmsopencvpluginsampleimpl)
    set (KMSOPENCVPLUGINSAMPLE_IMPLEMENTATION_EXTRA_INCLUDE_DIR "${KMSOPENCVPLUGINSAMPLE_SOURCE_DIR_PREFIX}/@_PARAM_SERVER_IMPL_LIB_EXTRA_HEADERS_PREFIX@")
  else ()
    find_path(KMSOPENCVPLUGINSAMPLE_IMPLEMENTATION_EXTRA_INCLUDE_DIR
      NAMES
        @_PARAM_SERVER_IMPL_LIB_EXTRA_HEADERS@
      PATH_SUFFIXES
        @_PARAM_SERVER_IMPL_LIB_EXTRA_HEADERS_PREFIX@
        kurento/modules/opencvpluginsample
    )
  endif ()

  list (APPEND KMSOPENCVPLUGINSAMPLE_INCLUDE_DIRS ${KMSOPENCVPLUGINSAMPLE_IMPLEMENTATION_EXTRA_INCLUDE_DIR})
  list (APPEND REQUIRED_VARS KMSOPENCVPLUGINSAMPLE_IMPLEMENTATION_EXTRA_INCLUDE_DIR)
endif ()

if (NOT "@_PARAM_INTERFACE_LIB_EXTRA_HEADERS@" STREQUAL "")
  if (TARGET kmsopencvpluginsampleinterface)
    set (KMSOPENCVPLUGINSAMPLE_INTERFACE_EXTRA_INCLUDE_DIR "${KMSOPENCVPLUGINSAMPLE_SOURCE_DIR_PREFIX}/@_PARAM_INTERFACE_LIB_EXTRA_HEADERS_PREFIX@")
  else ()
    find_path(KMSOPENCVPLUGINSAMPLE_INTERFACE_EXTRA_INCLUDE_DIR
      NAMES
        @_PARAM_INTERFACE_LIB_EXTRA_HEADERS@
      PATH_SUFFIXES
        @_PARAM_INTERFACE_LIB_EXTRA_HEADERS_PREFIX@
        kurento/modules/opencvpluginsample
    )
  endif()

  list (APPEND KMSOPENCVPLUGINSAMPLE_INCLUDE_DIRS ${KMSOPENCVPLUGINSAMPLE_INTERFACE_EXTRA_INCLUDE_DIR})
  list (APPEND REQUIRED_VARS KMSOPENCVPLUGINSAMPLE_INTERFACE_EXTRA_INCLUDE_DIR)
endif ()

if (TARGET kmsopencvpluginsampleimpl)
  set (KMSOPENCVPLUGINSAMPLE_LIBRARY kmsopencvpluginsampleimpl)
else ()
  find_library (KMSOPENCVPLUGINSAMPLE_LIBRARY
    NAMES
      kmsopencvpluginsampleimpl
    PATH_SUFFIXES
      ${KMSOPENCVPLUGINSAMPLE_BINARY_DIR_PREFIX}/src/server
  )
endif()

set (REQUIRED_LIBS "@REQUIRED_LIBS@")
foreach (LIB ${REQUIRED_LIBS})
  string(FIND ${LIB} " " POS)

  if (${POS} GREATER 0)
    string(REPLACE " " ";" REQUIRED_LIB_LIST ${LIB})
    include (CMakeParseArguments)
    cmake_parse_arguments("PARAM" "" "LIBNAME" "" ${REQUIRED_LIB_LIST})

    if (DEFINED PARAM_LIBNAME)
      generic_find (${REQUIRED_LIB_LIST} REQUIRED)
      set (LIB_NAME ${PARAM_LIBNAME})
    else()
      string (SUBSTRING ${LIB} 0 ${POS} LIB_NAME)
      string (SUBSTRING ${LIB} ${POS} -1 LIB_VERSION)
      string (STRIP ${LIB_NAME} LIB_NAME)
      string (STRIP ${LIB_VERSION} LIB_VERSION)
      generic_find (LIBNAME ${LIB_NAME} REQUIRED VERSION "${LIB_VERSION}")
    endif()
  else ()
    string (STRIP ${LIB} LIB_NAME)
    generic_find (LIBNAME ${LIB_NAME} REQUIRED)
  endif ()
  list (APPEND REQUIRED_LIBRARIES ${${LIB_NAME}_LIBRARIES})
  list (APPEND KMSOPENCVPLUGINSAMPLE_INCLUDE_DIRS ${${LIB_NAME}_INCLUDE_DIRS})

endforeach()

set(KMSOPENCVPLUGINSAMPLE_INCLUDE_DIRS
  ${KMSOPENCVPLUGINSAMPLE_INCLUDE_DIRS}
  CACHE INTERNAL "Include directories for KMSOPENCVPLUGINSAMPLE library" FORCE
)

set (KMSOPENCVPLUGINSAMPLE_LIBRARIES
  ${KMSOPENCVPLUGINSAMPLE_LIBRARY}
  ${KMSCORE_LIBRARIES}
  ${KMSELEMENTS_LIBRARIES}
  ${KMSFILTERS_LIBRARIES}
  ${REQUIRED_LIBRARIES}
  CACHE INTERNAL "Libraries for KMSOPENCVPLUGINSAMPLE" FORCE
)

include (FindPackageHandleStandardArgs)

find_package_handle_standard_args(KMSOPENCVPLUGINSAMPLE
  FOUND_VAR
    KMSOPENCVPLUGINSAMPLE_FOUND
  REQUIRED_VARS
    ${REQUIRED_VARS}
  VERSION_VAR
    KMSOPENCVPLUGINSAMPLE_VERSION
)

mark_as_advanced(
  KMSOPENCVPLUGINSAMPLE_FOUND
  KMSOPENCVPLUGINSAMPLE_VERSION
  KMSOPENCVPLUGINSAMPLE_INTERFACE_INCLUDE_DIR
  KMSOPENCVPLUGINSAMPLE_IMPLEMENTATION_INTERNAL_INCLUDE_DIR
  KMSOPENCVPLUGINSAMPLE_IMPLEMENTATION_GENERATED_INCLUDE_DIR
  KMSOPENCVPLUGINSAMPLE_IMPLEMENTATION_EXTRA_INCLUDE_DIR
  KMSOPENCVPLUGINSAMPLE_INTERFACE_EXTRA_INCLUDE_DIR
  KMSOPENCVPLUGINSAMPLE_INCLUDE_DIRS
  KMSOPENCVPLUGINSAMPLE_LIBRARY
  KMSOPENCVPLUGINSAMPLE_LIBRARIES
)
